<%= form_for [@user, @project] do |f| %>

  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>

  <div class="field">
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </div>

  <div class="field">
    <%= f.label :category %><br>
    <%= f.text_area :category %>
  </div>

  <div class="field">
    <%= f.label :website_url %><br>
    <%= f.text_field :website_url %>
  </div>

  <div class="field">
    <%= f.label :linkedin_url %><br>
    <%= f.text_field :linkedin_url %>
  </div>

  <div class="field">
    <%= f.label :twitter_url %><br>
    <%= f.text_field :twitter_url %>
  </div>

  <div class="field">
    <%= f.label :facebook_url %><br>
    <%= f.text_field :facebook_url %>
  </div>

  <div class="field">
    <%= f.label :instagram_url %><br>
    <%= f.text_field :instagram_url %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
