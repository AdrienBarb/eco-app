

<div id="users-index-wrap">
  <%= form_tag projects_path, role: 'search', method: :get, class: 'search-bar' do |f| %>
    <%= text_field_tag :q, params[:q], class: 'search-field' %>
    <button><i class="fas fa-search"></i></button>
  <% end %>


  <div id="users-index">
    <% @users.each do |user| %>
      <%= render 'shared/user-card', user: user %>
    <% end %>
  </div>


</div>




